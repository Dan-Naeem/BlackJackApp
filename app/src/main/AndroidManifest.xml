<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dan.blackjackltd">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SoloCampaign">
            <intent-filter>
                <action android:name="android.intent.action.Solo" />

            </intent-filter>
        </activity>
        <activity android:name=".CoopMission">
            <intent-filter>
                <action android:name="android.intent.action.Coop" />

            </intent-filter>
        </activity>
        <activity android:name=".StatsAct">
            <intent-filter>
                <action android:name="android.intent.action.Stats" />

            </intent-filter>
        </activity>
        <activity android:name=".LocalMatch">
            <intent-filter>
                <action android:name="android.intent.action.Local" />

            </intent-filter>
        </activity>
    </application>

</manifest>